title = hide-if-contains-image
template = testcase

<section class="site-panel">
  <h2>hide-if-contains-image</h2>
  <p>Check that basic usage of the hide-if-contains-image snippet is working as expected.</p>
  <p></p>
</section>

<section class="site-panel">
  <div id="target" class="testcase-container">
    <div class="testcase-content stripes-grey">Content</div>
    <div id="lbiddvhck3">
      <div style='margin: 10px auto; display: inline-block; width: 728px; height: 90px; background-image: url(data:image/png;base64,)'>Should not be hidden.</div>
    </div>
  </div>
  <h3>Filters</h3>
  <pre>{{ site_url|strip_proto }}#$#hide-if-contains-image /^89504e470d0a1a0a0000000d49484452000002d80000005a08/ div[shouldhide] div</pre>
</section>

<script>
  setTimeout(() =>
  {
    let ad = document.createElement("div");
    ad.id = Math.random().toString(36).substring(2);
    ad.setAttribute("shouldhide", "true");
    ad.innerHTML = "<div style='margin: 10px auto; display: inline-block; width: 728px; height: 90px; background-image: url(data:image/png;base64,)'>Should be hidden</div>";
    if (Math.random() < .5)
      document.getElementById("target").insertBefore(ad, document.getElementById("target").firstElementChild);
    else
      document.getElementById("target").appendChild(ad);
  },
  500);
</script>
